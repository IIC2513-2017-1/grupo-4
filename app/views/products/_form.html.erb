<%= simple_form_for product do |f| %>
    <%= f.input :sku %>
    <%= f.association :category %>
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :price %>
    <%= f.input :image, as: :file %>
    <%= f.input :jumbotron %>
    <%= f.input :jumbotron_image, as: :file %>
    <%= f.button :submit %>
<% end %>